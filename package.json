{
  "name": "simple-type-check",
  "version": "1.2.5",
  "description": "simple script to check type",
  "main": "dist/simpleTypeCheck.min.js",
  "scripts": {
    "build": "google-closure-compiler --js=src/index.js --js_output_file=dist/simpleTypeCheck.min.js --language_out=ECMASCRIPT3 --assume_function_wrapper",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "commit": "sgc",
    "test": "mocha \"test/**/*.spec.js\"",
    "lint": "eslint src/**/*.js test/**/*.js",
    "lint-fix": "eslint --fix src/**/*.js test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RaphaelHaettich/simple-type-check.git"
  },
  "keywords": [
    "type",
    "javascript",
    "check",
    "prop",
    "types",
    "check-types",
    "small",
    "type-check",
    "module"
  ],
  "private": false,
  "author": "Raphael HÃ¤ttich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RaphaelHaettich/simple-type-check/issues"
  },
  "homepage": "https://github.com/RaphaelHaettich/simple-type-check#readme",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/npm": "^5.1.1",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "google-closure-compiler": "^20181028.0.1",
    "jsdom": "^13.0.0",
    "mocha": "^5.2.0",
    "semantic-git-commit-cli": "^3.1.0",
    "semantic-release": "^15.12.1",
    "travis-deploy-once": "^5.0.9"
  }
}
